{
  "name": "react-async-stateful",
  "version": "0.1.2",
  "description": "Declarative async state to reduce boilerplate",
  "main": "index.js",
  "types": "index.d.ts",
  "esnext": "esm/index.js",
  "scripts": {
    "lint": "eslint --ext .tsx,.ts src",
    "lint:fix": "eslint --fix --ext .tsx,.ts src",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module es2015 --target es5 --outDir esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir cjs",
    "build:watch": "npm-run-all -p \"build:esm -- -w\" \"build:cjs -- -w\"",
    "examples:start": "parcel examples/index.html --out-dir ./public",
    "examples:build": "parcel build examples/index.html --out-dir ./public"
  },
  "keywords": [
    "typescript",
    "react",
    "async",
    "state",
    "hooks"
  ],
  "author": "James Waterhouse <09jwater@gmail.com>",
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "index.d.ts",
    "esm/",
    "cjs/",
    "react/",
    "redux/"
  ],
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-markdown-string": "^1.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.2.2",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "sass": "^1.23.7",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "repository": "https://github.com/Censkh/react-async-stateful"
}
